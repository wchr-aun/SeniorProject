		"autoprefixer": "^10.2.5",
		"commitizen": "^4.2.4",
		"cssnano": "^5.0.1",
		"cz-conventional-changelog": "3.3.0",
		"cz-emoji": "^1.3.1",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
